<div class="col-xs-6 col-xs-offset-3">
  <%= link_to "Back", edit_admin_camp_path(@day_schedule.camp) %>
  <hr>
  <%= simple_form_for @day_schedule, url: admin_day_schedule_path(@day_schedule) do |f| %>
    <%= f.input :date, as: :date, html5: true, disabled: true %>
    <%= f.input :content %>
    <%= f.button :submit, class: 'btn btn-primary' %>
  <% end %>
</div>
